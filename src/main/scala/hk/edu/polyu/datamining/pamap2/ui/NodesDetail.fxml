<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" hgap="14.0" maxHeight="+Infinity" maxWidth="+Infinity"
          minHeight="-Infinity" minWidth="-Infinity"
          vgap="20.0" xmlns="http://javafx.com/javafx/8.0.76-ea"
          fx:controller="hk.edu.polyu.datamining.pamap2.ui.NodesDetailController">
    <children>
        <VBox fx:id="main_vbox" maxHeight="+Infinity" maxWidth="+Infinity" minHeight="-Infinity" prefWidth="400.0"
              spacing="7.0" GridPane.rowIndex="0">
            <children>
                <Label fx:id="messageLabel" text="number of host" textAlignment="LEFT" wrapText="true">
                    <font>
                        <Font name="System Bold" size="13.0"/>
                    </font>
                </Label>
                <Label fx:id="detailsLabel" text="spare" textAlignment="LEFT" wrapText="true">
                    <font>
                        <Font size="12.0"/>
                    </font>
                </Label>
            </children>
        </VBox>
        <HBox alignment="BOTTOM_RIGHT" maxHeight="-Infinity" maxWidth="+Infinity" minHeight="-Infinity"
              minWidth="-Infinity" GridPane.rowIndex="1">
            <children>
                <Button minWidth="80.0" mnemonicParsing="false" onAction="#close_window" text="Ok" HBox.hgrow="NEVER">
                    <HBox.margin>
                        <Insets left="14.0"/>
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
    </children>
    <columnConstraints>
        <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" maxWidth="+Infinity" minWidth="-Infinity"/>
    </columnConstraints>
    <padding>
        <Insets bottom="14.0" left="14.0" right="14.0" top="14.0"/>
    </padding>
    <rowConstraints>
        <RowConstraints maxHeight="+Infinity" minHeight="-Infinity" valignment="CENTER" vgrow="ALWAYS"/>
        <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="NEVER"/>
    </rowConstraints>
</GridPane>
